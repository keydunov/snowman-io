<div class="row-fluid text-right">
  {{#link-to "apps.new" class="btn btn-default"}}Register App{{/link-to}}
</div>

<div class="row-fluid dashboard">
  {{#each app in model}}
    {{app-status app=app}}
  {{else}}
    <div>&nbsp;</div>
    <h1 class="text-center">
      You don't track any app yet, {{#link-to "apps.new"}}register one{{/link-to}}.
    </h1>
  {{/each}}
</div>
