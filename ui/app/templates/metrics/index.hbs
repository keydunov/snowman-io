<ol class="breadcrumb">
  <li class="active">Metrics</li>
</ol>

<div class="table-responsive">
<table class="table">
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Actions</th>
  </tr>
  {{#each metric in model}}
    <tr>
      <td>{{metric.name}}</td>
      <td>{{metric.lastValue}}</td>
      <td>{{#link-to "metrics/show" metric}}show{{/link-to}}</td>
    </tr>
  {{/each}}
</table>
</div>
